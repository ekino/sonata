{
  "name": "ekino/sonata",
  "description": "Sonata Helpers by Ekino",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Ekino PHP Team",
      "email": "php@ekino.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "sonata-project/admin-bundle": "^3.1",
    "sonata-project/doctrine-orm-admin-bundle": "^3.0",
    "sonata-project/page-bundle": "^3.8",
    "symfony/framework-bundle": "^3.0 || ^4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "phpunit/phpunit": "^6.0",
    "symfony/phpunit-bridge": "^4.0"
  },
  "autoload": {
    "psr-4": { "Sonata\\SonataHelpersBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Sonata\\SonataHelpersBundle\\Tests\\": "tests/" }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}
